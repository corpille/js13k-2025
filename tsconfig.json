{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "lib": ["esnext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false
  },
  "include": ["src", "assets"]
}